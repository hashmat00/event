<div id="tab2">
  <div class="col-xs-12 content_2">
    <div class="col-md-10 col-md-offset-1">
      <div class="wide-2">
        <div class="row">
          <% events.each do |i| %>
            <div class="col-md-3 col-sm-3 col-xs-6 prop">
              <div class="wht-cont">
                <div class="exp-img-2" style="background-image:url(<%= i.bgpicture_url %>);background-size: cover;">
                  <span class="filter"></span>
                  <span class="ffs-bs"><label for="op" class="btn btn-small btn-primary">browse photos</label></span>
                  <div class="overlay">
                    <div class="img-counter"><%= i.pictures.count %> Photo</div>
                  </div>
                </div>
                <div class="item-title">
                  <h4><a href="property_page.html"><%= i.name %></a></h4>
                  <p class="team-color"><%= i.address %></p>
                  <div class="col-md-7 col-sm-7 col-xs-7">
                    <p><%= render i.categories %></p>
                  </div>
                  <div class="col-md-5 col-sm-5 col-xs-5">
                    <p><%= i.distance_from([latlong[:lat],latlong[:long]]).round(2)%> Km <span class="rank"></span></p>
                  </div>
                  <div class="col-md-7 col-sm-7 col-xs-7 lft-brd"></div>
                  <div class="col-md-5 col-sm-5 col-xs-5 lft-brd"></div>
                </div>
                <hr>
                <div class="item-title btm-part">
                  <div class="row">
                    <div class="col-md-8 col-sm-8 col-xs-8">
                      <p>$<%= i.payMode("amount") rescue "Something Wrong" %></p>
                      <p class="team-color"><%= i.payMode("string") rescue "Something Wrong" %></p>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-4 favorite">
                      <div>
                        <a href="javascript:void(0)" data-toggle="modal" data-target="#myModal"><span class="glyphicon glyphicon-share" style="font-size:20px;font-weight:bold" title="Share"></span></a>
                        <a data-remote="true" href="<%=add_wishlist_event_path(i.id) %>"><span class="glyphicon <%= i.wish_list_class(current_user.id) %>" id="wish_list_<%= i.id %>" style="font-size:20px;font-weight:bold" title="Save" ></span></a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
      <!-- content_2 -->
    </div>
  </div>
</div>

<style>
  #page-content-search .container .wide_container_2 .tabs .tab-content {
    display: block !important;
}
</style>