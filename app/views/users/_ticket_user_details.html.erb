<div class="text-con">
  <div class="text-block">
    <p>Contact Information</p>
  </div>
  
    <div class="form-group">
      <label class="control-label col-sm-2" for="email">First Name</label>
      <div class="col-sm-10">
        <p><%= ticket.subscription.try(:payment).try(:first_name).try(:titleize) %></p>
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="pwd">Last Name</label>
      <div class="col-sm-10">          
        <p><%= ticket.subscription.try(:payment).try(:last_name).try(:titleize) %></p>
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="pwd">Email</label>
      <div class="col-sm-10">          
        <p><%= ticket.subscription.try(:payment).try(:email) %></p>
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="pwd">Contact </label>
      <div class="col-sm-10">          
        <p><%= ticket.subscription.try(:payment).try(:contact_no) %></p>
      </div>
    </div>
</div>