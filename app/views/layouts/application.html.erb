<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <meta name="google-site-verification" content="b6KspSExufhe85f_1xbprHpeaZwSBXNvsUcfjRmWXIw" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
 
  <% if (params[:controller] == "users" && (params[:action]=="tickets_history")) %>
  <link rel="stylesheet" type="text/css" href="https://cdn.evbstatic.com/s3-build/16702-rc2016-07-04_12.04-f38382e/django/css/production/base_styles.css" />
  <link rel="stylesheet" type="text/css" href="https://cdn.evbstatic.com/s3-build/16702-rc2016-07-04_12.04-f38382e/django/css/src/smacss/non_modular/global_header.css" />
  <link rel="stylesheet" type="text/css" href="https://cdn.evbstatic.com/s3-build/16702-rc2016-07-04_12.04-f38382e/django/css/production/responsive_mytickets.css" />
  <link rel="stylesheet" type="text/css" href="https://cdn.evbstatic.com/s3-build/16702-rc2016-07-04_12.04-f38382e/django/css/src/smacss/non_modular/user_profile.css" />
  <link rel="stylesheet" type="text/css" href="https://cdn.evbstatic.com/s3-build/16702-rc2016-07-04_12.04-f38382e/django/css/src/smacss/non_modular/mytickets_responsive.css" />
  <% end %>
  <script src="//cdn.ckeditor.com/4.5.10/standard/ckeditor.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/formvalidation/0.6.1/js/framework/bootstrap.min.js"></script> 
   <script src="http://github.com/jashkenas/coffee-script/raw/master/extras/coffee-script.js" type="text/javascript">
  </script>
  <% if (params[:controller] == "events" && (params[:action]=="new")) || (params[:controller] == "events" && (params[:action]=="edit")) %>  
    <link rel="stylesheet" type="text/css" href="https://cdn.evbstatic.com/s3-build/16833-rc2016-07-06_12.04-16f04f7/django/css/production/base_styles.css" />
  <% end %>

  <% if (params[:controller] == "events" && (params[:action]=="index")) || (params[:controller] == "events" && (params[:action]=="show")) %> 
  <link href="http://themes.fruitfulcode.com/suburb/assets/fonts/font-awesome.css" rel="stylesheet" type="text/css">
  <%= stylesheet_link_tag    'show_events', media: 'all' %>
  <script type="text/javascript" src="http://themes.fruitfulcode.com/suburb/assets/js/masonry.pkgd.min.js"></script>
  <script type="text/javascript" src="http://themes.fruitfulcode.com/suburb/assets/js/tmpl.js"></script>
  <script type="text/javascript" src="http://themes.fruitfulcode.com/suburb/assets/js/icheck.min.js"></script>
  <script type="text/javascript" src="http://themes.fruitfulcode.com/suburb/assets/js/draggable-0.1.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/jquery-ui.min.js"></script>
  <script type="text/javascript" src="http://themes.fruitfulcode.com/suburb/assets/js/jquery-migrate-1.2.1.min.js"></script>
  <script type="text/javascript" src="http://themes.fruitfulcode.com/suburb/assets/js/jquery.raty.min.js"></script>
  <% end %>
 <!--  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDl8KUzAOcfadtLw7_wkJ_NyoD5vDz3_74&callback=initMap&libraries=places" async defer></script> -->
  <!--  <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDl8KUzAOcfadtLw7_wkJ_NyoD5vDz3_74&callback=initMap">
  </script> -->
</head>
<body>  

<div class="ajax-loading">
  <span>
    <p id="please_wait"><strong>Please wait....</strong></p>
    <p><i class="glyphicon glyphicon-refresh gly-spin" aria-hidden="true"></i></p>
  </span>
</div>
<%= render 'layouts/navigation' %>
<br>
<!--navigation-->
<%= render 'layouts/messages' %>
<%= yield %>

<!--footer -->
<%= render 'layouts/footer' %>

</body>
</html>
